{
  "manifest_version": 3,
  "name": "Tabby",
  "version": "3.1.2",
  "description": "Organize your tabs into groups and manage them locally.",
  "permissions": ["tabs", "storage"],
  "host_permissions": ["<all_urls>"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png"
    }
  },
  "options_page": "options.html",
  "icons": {
    "16": "icon.png"
  }
}

